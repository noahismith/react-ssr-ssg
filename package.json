{
    "name": "static-site-generation",
    "version": "1.0.0",
    "description": "A barebones example of SSG using React, and Webpack",
    "main": "./server/index.js",
    "type": "module",
    "scripts": {
        "build:dev": "webpack --mode=development",
        "build:prod": "webpack --mode=production",
        "start": "npm run build:dev && nodemon dist/server.bundle.cjs",
        "generate": "npm run build:dev && node dist/prerender.bundle.cjs"
    },
    "author": "Noah Ignacio Smith",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.23.7",
        "@babel/preset-env": "^7.23.7",
        "@babel/preset-react": "^7.23.3",
        "@mdx-js/loader": "^3.0.0",
        "@mdx-js/react": "^3.0.0",
        "babel-loader": "^9.1.3",
        "express": "^4.18.2",
        "nodemon": "^3.0.2",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.21.1"
    }
}
